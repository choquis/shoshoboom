      "fullName": "Bounce object",
      "functionType": "Action",
      "name": "Bounce",
      "sentence": "Bounce _PARAM1_ off _PARAM2_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "SetNumberVariable"
              },
